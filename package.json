{
  "name": "csci-e39",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build/* public/*",
    "build.public.js": "webpack || exit 1",
    "build.public.css": "node-sass -r -o public/css src/assignments",
    "build.public": "mkdir -p public && npm run build.public.css && npm run build.public.js",
    "build.server": "mkdir -p build && babel -d ./build ./src -s && cp ./src/ui/index.pug ./build/ui",
    "build": "npm run clean && npm run build.server && npm run build.public",
    "knex": "babel-node node_modules/.bin/knex",
    "migration": "npm run knex migrate:make",
    "migrate": "npm run knex migrate:latest",
    "prestart": "npm run build",
    "start": "node build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run clean && npm run build.public && babel-node src/server.js",
    "watch.js": "nodemon --ext js,pug --watch ./src --exec 'npm run build.public.js && babel-node src/server.js'",
    "watch.css": "nodemon --ext css,scss --watch ./src --exec 'npm run build.public.css'",
    "watch.livereload": "livereload 'build, public'",
    "watch": "npm run clean && concurrently 'npm run watch.js' 'npm run watch.css' 'npm run watch.livereload'"
  },
  "author": "Tim Shelburne",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.18.0",
    "bookshelf": "^0.10.4",
    "class-autobind": "^0.1.4",
    "cloudinary": "^1.9.0",
    "debug": "^3.0.1",
    "js-beautify": "^1.7.5",
    "jsx-to-string": "^1.3.1",
    "kcors": "^2.2.1",
    "knex": "^0.13.0",
    "koa": "^2.3.0",
    "koa-logger": "^3.0.1",
    "koa-route": "^3.2.0",
    "koa-static": "^4.0.1",
    "koa-views": "^6.0.2",
    "pg": "^7.2.0",
    "prop-types": "^15.5.10",
    "pug": "^2.0.0-rc.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-fontawesome": "^1.6.1",
    "socket.io": "^2.0.3",
    "supports-color": "^4.4.0",
    "xstream": "^10.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "concurrently": "^3.5.0",
    "livereload": "^0.6.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.11.0",
    "sqlite3": "3.1.8",
    "webpack": "^3.5.5"
  }
}
